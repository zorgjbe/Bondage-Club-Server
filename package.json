{
	"name": "BondageClubServer",
	"version": "1.0.0",
	"author": "Ben987",
	"engines": {
		"node": "14.x"
	},
	"repository": "https://github.com/GoogleCloudPlatform/nodejs-getting-started",
	"main": "app.js",
	"private": true,
	"scripts": {
		"start": "node --unhandled-rejections=throw app.js",
		"start:debug": "node --unhandled-rejections=throw --inspect=0.0.0.0:9229 app.js",
		"check": "tsc --allowJs --checkJs --noEmit app.js",
		"test": "jest",
		"cover": "jest --coverage",
		"docker:start": "docker-compose up -d --build",
		"docker:stop": "docker-compose down",
		"docker:restart": "npm run docker:stop && npm run docker:start",
		"docker:logs": "docker-compose logs -f app db"
	},
	"dependencies": {
		"base64id": "^2.0.0",
		"bcrypt": "^5.0.1",
		"heroku": "^7.66.4",
		"mongodb": "^3.4.1",
		"newrelic": "^7.1.2",
		"nodemailer": "^6.6.1",
		"socket.io": "^3.1.2",
		"sync-request": "^6.1.0"
	},
	"devDependencies": {
		"@types/jest": "^29.2.5",
		"@types/mongodb": "^3.4.1",
		"dotenv": "^10.0.0",
		"dotenv-expand": "^5.1.0",
		"jest": "^29.3.1",
		"socket.io-client": "^4.4.0",
		"ts-jest": "^29.0.5",
		"typescript": "^4.9.4",
		"socket.io-client": "^4.4.0"
	}
}
